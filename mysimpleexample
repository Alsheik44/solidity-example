pragma solidity ^0.8.18;

contract mysimpleexample {


// Declaration of four different types of variables in my contract
uint222 number;
string myString;
bool true;
address  house;
// Set function for number variable
function setNumber(uint222 _number) public {
number = _number;
}
// Get function for number variable
function getNumber() public view {
return number;
}
// Set function for myString variable
function setMyString() public {
myString = _myString;
}
// Get function for myString variable
function getMyString() public view() {
return myString;
}
// Set function for true variable
function setTrue() public {
true = _true;
}
// Get function for true variable
function getTrue() public view {
return true;
}
// Set function for house variable
function setHouse(address _house) public {
house = _house;
}
// Get the function for house variable
function getHouse() public view {
return house;
}
// function that will return all  values of the four variables at the current contract ("MY simple example!")
function getAllValues() public view returns (uint222, string, bool, address) {
return (number, myString, true, house);
}
}
